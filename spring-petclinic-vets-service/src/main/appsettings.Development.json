{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "spring:": {
    "cloud": {
      "config": {
        "uri": "http://localhost:8888"
      }
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka"
    }
  },

  "management": {
    "tracing": {
      "exporter": {
        "zipkin": {
          "endpoint": "http://localhost:9411/api/v2/spans",
          "validateCertificates": false,
          "useShortTraceIds ": true
        }
      }
    }
  }
}
